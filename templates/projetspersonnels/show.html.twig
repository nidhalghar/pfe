{% extends 'base.certificat.html.twig' %}
{% block image %}{{asset(app.user.image)}}{% endblock %}
{% block title %}Projetspersonnel{% endblock %}

{% block body %}
   
<div class="card">
    <div class="card-header">
        <h2 class="text-center">Projetspersonnel</h2>
    </div>
    <div class="card-body">
        <table class="table">
            <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ projetspersonnel.id }}</td>
                </tr>
                <tr>
                    <th>Idee</th>
                    <td>{{ projetspersonnel.idee }}</td>
                </tr>
                <tr>
                    <th>Duree</th>
                    <td>{{ projetspersonnel.duree }}</td>
                </tr>
                <tr>
                    <th>Technologies</th>
                    <td>{{ projetspersonnel.technologies }}</td>
                </tr>
                <tr>
                    <th>Description</th>
                    <td>{{ projetspersonnel.description }}</td>
                </tr>
            </tbody>
        </table>
        <div class="text-center">
            <a href="{{ path('app_projetspersonnels_index') }}" class="btn btn-primary mx-2">
                <i class="fas fa-arrow-left"></i> Back to list
            </a>
            <a href="{{ path('app_projetspersonnels_edit', {'id': projetspersonnel.id}) }}" class="btn btn-warning mx-2">
                <i class="fas fa-edit"></i> Edit
            </a>
            {{ include('projetspersonnels/_delete_form.html.twig') }}
        </div>
    </div>
</div>
{% endblock %}
